# Example environment variables for local development or Render
# Copy this file to `.env` (locally) and fill values as needed.

# Django settings
SECRET_KEY=replace-me-with-a-secret
DEBUG=False
ALLOWED_HOSTS=127.0.0.1,.onrender.com

# WSGI module for Procfile / Render
WSGI_MODULE=Cloud.wsgi

# Postgres database URL (example format)
# postgres://USER:PASSWORD@HOST:PORT/DBNAME
DATABASE_URL=postgres://postgres:postgres@127.0.0.1:5432/CloudHealth

# Set to True to disable SSL requirement when connecting to DB (not recommended)
DISABLE_DB_SSL=False